{
  "name": "@intric/web",
  "version": "1.23.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "NODE_ENV=production vite build",
    "build:all": "pnpm run --recursive --filter @intric/web... build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "test": "npm run test:integration && npm run test:unit",
    "check": "NODE_ENV=production svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "i18n:compile": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide"
  },
  "devDependencies": {
    "@internationalized/date": "^3.7.0",
    "@intric/eslint-plugin": "workspace:*",
    "@intric/intric-js": "workspace:*",
    "@intric/ui": "workspace:*",
    "@melt-ui/svelte": "^0.86.5",
    "@playwright/test": "^1.51.1",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-cloudflare": "^7.0.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-vercel": "^5.7.0",
    "@sveltejs/kit": "^2.20.5",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.1.3",
    "@types/node": "^22.14.0",
    "@types/wicg-file-system-access": "^2023.10.6",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.13",
    "eslint": "^9.24.0",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^5.25.12",
    "svelte-check": "^4.1.5",
    "svelte-eslint-parser": "^1.1.2",
    "svelte-headless-table": "^0.18.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "uid": "^2.0.2",
    "vite": "^6.3.0",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.0.13"
  }
}
